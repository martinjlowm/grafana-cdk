    "@grafana/schema": "^11.4.0",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "typescript": "^5.7.2"
